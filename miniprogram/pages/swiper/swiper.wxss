/* login页面 */
page{
  height: 100%;
}
.login-container {
	height: 100%;
	width: 100%;
	padding: 10px 30px;
	background-color: white;
}

.app-info {
	position: relative;
	padding: 40px;
	text-align: center;
}

.app-info:after {
	content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #E5E5E5;
  color: #E5E5E5;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.app-info .app-logo {
	display: block;
	width: 90px;
	height: 90px;
	margin: 10px auto;
	border-radius: 50%;
}

.app-info .app-name {
	/* font-weight: bold; */
	font-size: 12px;
	color: #000;
}

.alert {
	margin: 20px 0 30px;
}

.alert .alert-title {
	font-weight: 400;
	font-size: 16px;
	color: #000;
	margin-bottom: 10px;
}

.alert-desc {
	display: block;
	list-style: disc inside none;
}

.alert .alert-text {
	display: list-item;
    text-align: -webkit-match-parent;
	font-size: 14px;
	color: #999;
}

.logged {
	margin-top: 100px;
}

.logged .logged-icon {
	display: block;
	width: 64px;
	height: 64px;
	margin: 20px auto;
}

.logged .logged-text {
	font-size: 14px;
	color: #000;
	text-align: center;
	margin: 10px 0;
}